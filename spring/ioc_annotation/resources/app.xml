<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">

   <context:component-scan base-package="com.wangrui.ioc_annotation"></context:component-scan>
   
    <bean name="java_Book" class="com.wangrui.ioc_annotation.Book">
        <property name="name" value="Java"></property>
    </bean>
    <bean name="jsBook" class="com.wangrui.ioc_annotation.Book">
        <property name="name" value="JavaScript"></property>
    </bean>
    
<!--     <bean name="service" class="com.wangrui.ioc_annotation.ServiceA" init-method="init" destroy-method="destory"></bean> -->
</beans>
