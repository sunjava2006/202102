package com.thzhima.base.work;

public class ExampleFor {

	public static void main(String[] args) {
		/*
		 * 打印输出：
		 *    **********
		 *    **********
		 *    **********
		 *    **********
		 *    **********
		 */
		
		for(int row = 1; row <= 5; row++) {
			for(int count = 1;count<=10; count++) {
				System.out.print("*");
			}
			System.out.println();
			
		}
		
		/*
		 * 打印输出：
		 *    **********
		 *     **********
		 *      **********
		 *       **********
		 *        **********
		 */
		
		for(int i=0; i<5; i++) {
			for(int b=0; b<i; b++) {
				System.out.print(" ");
			}
			for(int i=1; i<=10; i++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
		/*
		 * 输出 99 乘法表
		 */
		
		
		/*
		 * *
		 * **
		 * ***
		 * ****
		 * *****
		 */
		
		
	}
}
